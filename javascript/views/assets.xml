<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <template id="assets_backend" name="backend js" inherit_id="web.assets_backend">
       <xpath expr="." position="inside">
           <script type="text/javascript" src="/javascript/static/src/js/backend.js"/>
           <script type="text/javascript" src="javascript/static/src/js/student_record.js"/>
       </xpath>
   </template>
   <template id="assets_frontend" inherit_id="web.assets_frontend">
      <xpath expr="." position="inside">
          <link rel="stylesheet" href="/javascript/static/src/css/style.css"/>
      </xpath>
</template>
</odoo>
